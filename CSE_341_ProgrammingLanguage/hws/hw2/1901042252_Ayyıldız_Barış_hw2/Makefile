PARSER_FILE = gpp_interpreter.y
LEXER_FILE = gpp_interpreter.l
LISP_FILE = gpp_interpreter.lisp
INPUT_FILE = input.txt

compile:
	echo "Outputs in the terminal generated by YACC\n"
	echo "Outputs in the output.txt generated by Lisp\n"
	yacc -d $(PARSER_FILE)
	lex $(LEXER_FILE)
	gcc lex.yy.c y.tab.c -w
	./a.out < $(INPUT_FILE)
	clisp $(LISP_FILE) $(INPUT_FILE)
